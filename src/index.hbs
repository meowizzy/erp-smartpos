<!doctype html>
<html lang="{{ lang }}">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{{ title }}</title>
    <link rel="icon" type="image/png" sizes="16x16" href="./assets/favicon.ico">
</head>
<body>
    <svg style="position: absolute; top: -1000px; left: -1000px; pointer-events: none; opacity: 0">
        {{> template-parts/sprites }}
    </svg>
    {{> template-parts/header }}
    {{> template-parts/banner }}
    {{> template-parts/advantages }}
    {{> template-parts/possibilities }}
    {{> template-parts/partners }}
    {{> template-parts/tabs }}
    {{> template-parts/webKassa }}
    {{> template-parts/infoBlock }}
    {{> template-parts/slider }}
    {{> template-parts/callback-form }}
    {{> template-parts/footer }}
    {{> template-parts/modal type="modal"}}
    {{> template-parts/modal type="drawer"}}
</body>
</html>